#!/usr/bin/perl
# ABSTRACT: Various utilities for App::Spec files
# vim:et:sts=4:sws=4:sw=4
use strict;
use warnings;
use 5.010;
use Data::Dumper;
use FindBin '$Bin';

our $VERSION = '0.000'; # VERSION

use App::Spec;
use App::Spec::Bash;
my $specfile = "$Bin/../share/appspec-bash.yaml";

my $spec = App::Spec->read($specfile);
my $run = $spec->runner;
$run->run;

