#!/usr/bin/perl
# ABSTRACT: Various utilities for App::Spec files
# vim:et:sts=4:sws=4:sw=4
use strict;
use warnings;
use 5.010;
use Data::Dumper;
use FindBin '$Bin';

our $VERSION = '0.000'; # VERSION

use App::Spec;
use App::Spec::Bash;
use App::Spec::Bash::Spec qw/ $SPEC /;

my $spec = App::Spec->read($SPEC);
my $run = $spec->runner;
$run->run;

